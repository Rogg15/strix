        if v == true then
            
            pcall(function()
                for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
                    if v.Name == 'BrightFlashlight' or v.Name == 'Flashlight' then
                        v.Parent = game.Players.LocalPlayer.Character
                    end
                end
            end)
    
            LP = game:GetService("Players").LocalPlayer
            for i,v in pairs(LP.Character:GetDescendants()) do
            if v:IsA("Tool") then
            v.Parent = LP
            end
            end

            fireclickdetector(game:GetService("Workspace").StuffGiversFolder.MegaFishCoalParts["CoalPart1"].ClickDetector)
            wait(0.1)
            fireclickdetector(game:GetService("Workspace").StuffGiversFolder.MegaFishCoalParts["CoalPart4"].ClickDetector)
            wait(0.1)
            fireclickdetector(game:GetService("Workspace").StuffGiversFolder.MegaFishCoalParts["CoalPart2"].ClickDetector)
            wait(0.1)
            fireclickdetector(game:GetService("Workspace").StuffGiversFolder.MegaFishCoalParts["CoalPart3"].ClickDetector)
            wait(0.1)
            fireclickdetector(game:GetService("Workspace").StuffGiversFolder.MegaFishCoalParts["CoalPart6"].ClickDetector)
            wait(0.1)
            fireclickdetector(game:GetService("Workspace").StuffGiversFolder.MegaFishCoalParts["CoalPart5"].ClickDetector)
            wait(0.1)
            fireclickdetector(game:GetService("Workspace").StuffGiversFolder.MegaFishCoalParts["CoalPart7"].ClickDetector)
            wait(0.1)
            fireclickdetector(game:GetService("Workspace").StuffGiversFolder.MegaFishCoalParts["CoalPart8"].ClickDetector)
            wait(1)
            game:GetService('RunService'):BindToRenderStep("Killrake", 0 , function(v)  
                
                for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
                    if v:IsA("Tool") then
                    v.Parent = game.Players.LocalPlayer.Character
                    end
                end
                 wait(.1)
                    if game.Players.LocalPlayer.Character:FindFirstChild("MegaFish") then
                        game.Players.LocalPlayer.Character.Humanoid:UnequipTools(game.Players.LocalPlayer.Character:FindFirstChild("MegaFish"))
                    end
       
    
             if game.Players.LocalPlayer.Backpack:FindFirstChild("MegaFish") then
                    game.Players.LocalPlayer.Character.Humanoid:EquipTool(game.Players.LocalPlayer.Backpack:FindFirstChild("MegaFish"))
                end
    
    
              if game.Players.LocalPlayer.Character:FindFirstChild("MegaFish") then
                    game.Players.LocalPlayer.Character.Humanoid:UnequipTools(game.Players.LocalPlayer.Character:FindFirstChild("MegaFish"))
                end
    
                if game.Players.LocalPlayer.Character:FindFirstChild("MegaFish") then
                    game.Players.LocalPlayer.Character.Humanoid:UnequipTools(game.Players.LocalPlayer.Character:FindFirstChild("MegaFish"))
                end
               
                         if game.Players.LocalPlayer.Backpack:FindFirstChild("Pan") then
                    game.Players.LocalPlayer.Character.Humanoid:EquipTool(game.Players.LocalPlayer.Backpack:FindFirstChild("MegaFish"))
                         end
            wait()
            if game.Players.LocalPlayer.Character:FindFirstChild("Pan") then
                game.Players.LocalPlayer.Character.Humanoid:UnequipTools(game.Players.LocalPlayer.Character:FindFirstChild("Pan"))
            end
                    end)
        elseif v == false then
            game:GetService('RunService'):UnbindFromRenderStep("Killrake")
        end
     end
     })









     local column = tabsection:AddColumn({
        Title = " LocalPlayer"
     })
     
     local section = column:Section({
        Title = ""
     })




     section:Slider({Text = "Walkspeed",Min = 0, Max = 190,Def = 50,Callback = function(s) 
        game.Players.LocalPlayer.Character:FindFirstChildWhichIsA('Humanoid').Name = 'XCO'
    game.Players.LocalPlayer.Character:FindFirstChildWhichIsA('Humanoid').WalkSpeed = s
    if s == 16 then
        game.Players.LocalPlayer.Character:FindFirstChildWhichIsA('Humanoid').Name = 'XCO'
        game.Players.LocalPlayer.Character:FindFirstChildWhichIsA('Humanoid').WalkSpeed = 16
    end
    end
     })   

     section:Slider({Text = "Jumpower",Min = 0, Max = 500,Def = 50,Callback = function() 
        game.Players.LocalPlayer.Character:FindFirstChildWhichIsA('Humanoid').Name = 'XCO'
    game.Players.LocalPlayer.Character:FindFirstChildWhichIsA('Humanoid').JumpPower = s
    if s == 16 then
        game.Players.LocalPlayer.Character:FindFirstChildWhichIsA('Humanoid').Name = 'XCO'
        game.Players.LocalPlayer.Character:FindFirstChildWhichIsA('Humanoid').JumpPower = 16
    end
     end
      })   







    end




































































































































































































































































































    






