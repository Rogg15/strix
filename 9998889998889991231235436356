                return amt:reverse():gsub("%d%d%d", "%1,"):reverse():gsub("^,", "")
            end
            game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer("$"..fornum(amt), 'All')
        
    
