-- Gui to Lua
-- Version: 3.2

-- Instances:

local ScreenGui = Instance.new("ScreenGui")
local Frame = Instance.new("Frame")
local tbox = Instance.new("TextBox")
local submit = Instance.new("TextButton")

--Properties:

ScreenGui.Parent = game.CoreGui

Frame.Parent = ScreenGui
Frame.BackgroundColor3 = Color3.fromRGB(65, 65, 65)
Frame.BorderColor3 = Color3.fromRGB(0, 0, 0)
Frame.Position = UDim2.new(0.374384224, 0, 0.288723677, 0)
Frame.Size = UDim2.new(0, 265, 0, 49)
Frame.Draggable = true
Frame.Selectable = true
Frame.Active = true


tbox.Name = "tbox"
tbox.Parent = Frame
tbox.BackgroundColor3 = Color3.fromRGB(100, 100, 100)
tbox.BorderColor3 = Color3.fromRGB(0, 0, 0)
tbox.Position = UDim2.new(0.0441899151, 0, 0.130434617, 0)
tbox.Size = UDim2.new(0, 190, 0, 36)
tbox.Font = Enum.Font.SourceSans
tbox.Text = "Sound ID"
tbox.TextColor3 = Color3.fromRGB(0, 0, 0)
tbox.TextSize = 14.000

submit.Name = "submit"
submit.Parent = Frame
submit.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
submit.BorderColor3 = Color3.fromRGB(0, 0, 0)
submit.Position = UDim2.new(0.803773582, 0, 0.130434617, 0)
submit.Size = UDim2.new(0, 43, 0, 35)
submit.Font = Enum.Font.SourceSans
submit.Text = "Submit"
submit.TextColor3 = Color3.fromRGB(255, 0, 0)
submit.TextSize = 14.000

function sound(s)
	local newsound = Instance.new("Sound",workspace)
	newsound.Name = "earbleed"
	newsound.SoundId = "rbxassetid://"..tbox.Text
	newsound.Looped = true
	newsound:Play() 
	local earbleep = Instance.new("DistortionSoundEffect",newsound)
	earbleep.Enabled = false
	earbleep.Level = 10
	earbleep.Priority = 5

	local earbleep= Instance.new("EqualizerSoundEffect",newsound)
	earbleep.Enabled = false
	earbleep.HighGain = -80
	earbleep.LowGain = 10
	earbleep.MidGain = 1
	earbleep.Priority = 0
	game.Workspace.earbleed.Volume = 10


end
submit.MouseButton1Down:connect(sound)

game.CoreGui.ScreenGui.ResetOnSpawn = false

end)
MainSection:NewSlider("Volume", "SliderInfo", 10, 0, function(s)
   game.Workspace.earbleed.Volume = s

end)


MainSection:NewButton("Normal", "ButtonInfo", function(v)

game.Workspace.earbleed.DistortionSoundEffect.Enabled = false
game.Workspace.earbleed.EqualizerSoundEffect.Enabled = false

end)

MainSection:NewButton("NormalBass", "ButtonInfo", function(v)

game.Workspace.earbleed.DistortionSoundEffect.Enabled = false
game.Workspace.earbleed.EqualizerSoundEffect.Enabled = true

end)

MainSection:NewButton("BassBoosted", "ButtonInfo", function(v)

game.Workspace.earbleed.DistortionSoundEffect.Enabled = true
game.Workspace.earbleed.EqualizerSoundEffect.Enabled = true


end)


MainSection:NewButton("Stop Music", "ButtonInfo", function(v)
 game.Workspace.earbleed:Destroy()
    
    game.Workspace.earbleed.DistortionSoundEffect:Destroy()
    
    
end)

