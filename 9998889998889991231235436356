    game.Workspace.CurrentCamera.CameraSubject = game.Players.LocalPlayer.Character.Humanoid
     end
     })
  

     local section = column:Section({
        Title = "FlingStuff"
     })


section:Textbox({Text = "Fling",Callback = function(txt)
    local Target = gplr(txt)
    if Target[1] then
        Target = Target[1]

        local Loop
        local OldFlingPos = player.Character.HumanoidRootPart.Position
        local loopFunction = function()

            local success,err = pcall(function()
                local FlingEnemy = Target.Character
                if FlingEnemy and player.Character then
                    FlingTorso = FlingEnemy.UpperTorso
                    local dis = 3.85
                    local increase = 6
                    if FlingEnemy.Humanoid.MoveDirection.X < 0 then
                        xchange = -increase
                    elseif FlingEnemy.Humanoid.MoveDirection.X > 0  then
                        xchange = increase
                    else
                        xchange = 0
                    end
                    if FlingEnemy.Humanoid.MoveDirection.Z < 0 then
                        zchange = -increase
                    elseif FlingEnemy.Humanoid.MoveDirection.Z > 0 then
                        zchange = increase
                    else
                        zchange = 0
                    end          
                    if player.Character then
                        player.Character:FindFirstChildWhichIsA('Humanoid'):ChangeState(11)
                        player.Character.HumanoidRootPart.CFrame = CFrame.new(FlingTorso.Position.X + math.random(-dis,dis) + xchange, FlingTorso.Position.Y, FlingTorso.Position.Z + math.random(-dis,dis) + zchange) * CFrame.Angles(math.rad(player.Character.HumanoidRootPart.Orientation.X + 350),math.rad(player.Character.HumanoidRootPart.Orientation.Y + 200),math.rad(player.Character.HumanoidRootPart.Orientation.Z + 240))
                        player.Character.HumanoidRootPart.Velocity = Vector3.new(50000000,50000000,50000000)
                    end
                end
            end)
            if err then
                print('fling error : ' .. err)
            end
        end;
        local Start = function()    
            OldFlingPos = player.Character.HumanoidRootPart.Position
            Loop = game:GetService("RunService").Heartbeat:Connect(loopFunction);
        end;
        local Pause = function()
            Loop:Disconnect()
            local vectorZero = Vector3.new(0, 0, 0)
            player.Character.PrimaryPart.Velocity = vectorZero
            player.Character.PrimaryPart.RotVelocity = vectorZero
            player.Character.HumanoidRootPart.CFrame = CFrame.new(OldFlingPos) * CFrame.Angles(math.rad(0),math.rad(137.92),math.rad(0))
        local vectorZero = Vector3.new(0, 0, 0)
        player.Character.PrimaryPart.Velocity = vectorZero
        player.Character.PrimaryPart.RotVelocity = vectorZero
        player.Character.HumanoidRootPart.CFrame = CFrame.new(OldFlingPos) * CFrame.Angles(math.rad(0),math.rad(137.92),math.rad(0))
    end;

        Start()
        wait(10)
        Pause()

        



        
    end

    end
     })






















section:Textbox({Text = "BikeFling",Callback = function(txt)
        
        local Target = gplr(txt)
        if Target[1] then
            Target = Target[1]
            
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-832.8922119140625, 21.999820709228516, -546.8743286132812)
        wait(0.5)
        fireclickdetector(game:GetService("Workspace").Ignored.Shop["[DuoBike] - $75"].ClickDetector)
        fireclickdetector(game:GetService("Workspace").Ignored.Shop["[DuoBike] - $75"].ClickDetector)
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-832.6552124023438, 26.234392166137695, -553.2313842773438)
        
        tweenService, tweenInfo = game:GetService("TweenService"), TweenInfo.new(0.5, Enum.EasingStyle.Linear)
        tweenService:Create(game:GetService("Players")["LocalPlayer"].Character.HumanoidRootPart, tweenInfo, {CFrame = CFrame.new(-832.359375, 21.55292320251465, -552.5506591796875)}):Play()
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-832.6552124023438, 26.234392166137695, -553.2313842773438)
        wait(0.4)
        tweenService, tweenInfo = game:GetService("TweenService"), TweenInfo.new(0.5, Enum.EasingStyle.Linear)
        tweenService:Create(game:GetService("Players")["LocalPlayer"].Character.HumanoidRootPart, tweenInfo, {CFrame = CFrame.new(-832.359375, 21.55292320251465, -552.5506591796875)}):Play()
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-832.6552124023438, 26.234392166137695, -553.2313842773438)
        wait(0.8)
        tweenService, tweenInfo = game:GetService("TweenService"), TweenInfo.new(0.5, Enum.EasingStyle.Linear)
        tweenService:Create(game:GetService("Players")["LocalPlayer"].Character.HumanoidRootPart, tweenInfo, {CFrame = CFrame.new(-832.359375, 21.55292320251465, -552.5506591796875)}):Play()
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-832.6552124023438, 26.234392166137695, -553.2313842773438)
        wait(0.8)
        tweenService, tweenInfo = game:GetService("TweenService"), TweenInfo.new(0.5, Enum.EasingStyle.Linear)
        tweenService:Create(game:GetService("Players")["LocalPlayer"].Character.HumanoidRootPart, tweenInfo, {CFrame = CFrame.new(-832.359375, 21.55292320251465, -552.5506591796875)}):Play()
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-832.6552124023438, 26.234392166137695, -553.2313842773438)
        
                            game:FindService("RunService").Heartbeat:wait()
                            game.Players.LocalPlayer.Character:FindFirstChildWhichIsA('Humanoid'):ChangeState(11)
        local Loop
        local OldFlingPos = player.Character.HumanoidRootPart.Position
        local loopFunction = function()

            local success,err = pcall(function()
                local FlingEnemy = Target.Character
                if FlingEnemy and player.Character then
                    FlingTorso = FlingEnemy.UpperTorso
                    local dis = 3.85
                    local increase = 6
                    if FlingEnemy.Humanoid.MoveDirection.X < 0 then
                        xchange = -increase
                    elseif FlingEnemy.Humanoid.MoveDirection.X > 0  then
                        xchange = increase
                    else
                        xchange = 0
                    end
                    if FlingEnemy.Humanoid.MoveDirection.Z < 0 then
                        zchange = -increase
                    elseif FlingEnemy.Humanoid.MoveDirection.Z > 0 then
                        zchange = increase
                    else
                        zchange = 0
                    end          
                    if player.Character then
                        player.Character:FindFirstChildWhichIsA('Humanoid'):ChangeState(11)
                        player.Character.HumanoidRootPart.CFrame = CFrame.new(FlingTorso.Position.X + math.random(-dis,dis) + xchange, FlingTorso.Position.Y, FlingTorso.Position.Z + math.random(-dis,dis) + zchange) * CFrame.Angles(math.rad(player.Character.HumanoidRootPart.Orientation.X + 350),math.rad(player.Character.HumanoidRootPart.Orientation.Y + 200),math.rad(player.Character.HumanoidRootPart.Orientation.Z + 240))
                        player.Character.HumanoidRootPart.Velocity = Vector3.new(100000000,100000000,100000000)
                    end
                end
            end)
            if err then
                print('fling error : ' .. err)
            end
        end;
        local Start = function()    
            OldFlingPos = player.Character.HumanoidRootPart.Position
            Loop = game:GetService("RunService").Heartbeat:Connect(loopFunction);
        end;
        local Pause = function()
            Loop:Disconnect()
            local vectorZero = Vector3.new(0, 0, 0)
            player.Character.PrimaryPart.Velocity = vectorZero
            player.Character.PrimaryPart.RotVelocity = vectorZero
            player.Character.HumanoidRootPart.CFrame = CFrame.new(OldFlingPos) * CFrame.Angles(math.rad(0),math.rad(137.92),math.rad(0))
        local vectorZero = Vector3.new(0, 0, 0)
        player.Character.PrimaryPart.Velocity = vectorZero
        player.Character.PrimaryPart.RotVelocity = vectorZero
        player.Character.HumanoidRootPart.CFrame = CFrame.new(OldFlingPos) * CFrame.Angles(math.rad(0),math.rad(137.92),math.rad(0))
    end;

        Start()
        wait(10)
        Pause()

    local localPlayer = game:GetService('Players').LocalPlayer;
    local localCharacter = localPlayer.Character;
    localCharacter:FindFirstChildOfClass('Humanoid').Health = 0;
    for _, v in pairs(localPlayer.Character:GetChildren()) do
    if v.Name ~= 'BodyEffects' and v.Name ~= 'FULLY_LOADED_CHAR' then
        v:Destroy();
    end;
    end;
    wait(7)
    end



        end
    })



section:Button({Text = "StopFlingging",Callback = function() 
        local Pos = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
    
        for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
        if v:IsA('MeshPart') or v:IsA('Part') or v:IsA('Accessory') then
            v:Destroy()
        end
    end
    game.Players.LocalPlayer.Character.Name = 'Deleted'
    wait(6)
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = Pos
    end
    })




  local section = column:Section({
    Title = "Bags Target"
 })

section:Textbox({Text = "Bag Person",Callback = function(txt) 
    local plr = GetPlayer(txt)
    if plr then

        function getRoot(char)
            local rootPart = char:FindFirstChild('HumanoidRootPart') or char:FindFirstChild('Torso') or char:FindFirstChild('UpperTorso')
            return rootPart
        end
        if plr.name and game.Players[plr.name].Character.BodyEffects['K.O'].Value == false then
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Workspace.Ignored.Shop['[BrownBag] - $25'].Head.CFrame
            wait(.30)
            fireclickdetector(game.Workspace.Ignored.Shop['[BrownBag] - $25'].ClickDetector)
            game.Players.LocalPlayer.Backpack:WaitForChild("[BrownBag]").Parent = game.Players.LocalPlayer.Character
            local c = "Attempting to Bag  " .. plr.name .. "." 
            game.StarterGui:SetCore("SendNotification", {
                Title = "Notification";
                Text = c;
                Duration = 2;
            })
            wait(.5)
            repeat
                pcall(function()
                    wait()
                    getRoot(game.Players[plr.name].Character).CFrame = getRoot(game.Players.LocalPlayer.Character).CFrame + Vector3.new(1,3,0)
                    game.Players.LocalPlayer.Character["[BrownBag]"]:Activate()
                end)
            until game.Players[plr.name].Character:FindFirstChild("Christmas_Sock")
            local c = "Successfully Bagged  " .. plr.name .. "."
             game.StarterGui:SetCore("SendNotification", {
                Title = "Notification";
                Text = c;
                Duration = 2;
            })
        elseif game.Players[plr.name].Character.BodyEffects['K.O'].Value == false then
            local c = "" .. plr.name .. " Is Already Bagged." 
            game.StarterGui:SetCore("SendNotification", {
                Title = "Notification";
                Text = c;
                Duration = 2;
            })
        end
    end
    end
    })


  local section = column:Section({
    Title = "Locks Target"
 })



section:Textbox({Text = "Lock",Callback = function(txt) 
    local plr = GetPlayer(txt)
    if plr then

        local localPlayer     = game:GetService("Players").LocalPlayer;
         local localCharacter  = localPlayer.Character;
         
         if plr.name and game.Players[plr.name].Character.BodyEffects['K.O'].Value == false then
             pcall(function()
                 localCharacter.LeftHand.LeftWrist:Destroy();
                 localCharacter.RightHand.RightWrist:Destroy();
             end);
             repeat
                 wait();
                 localCharacter.LeftHand.CFrame = game.Players[plr.name].Character.LowerTorso.CFrame;
                 localCharacter.RightHand.CFrame = game.Players[plr.name].Character.LowerTorso.CFrame;
                 game.Players.LocalPlayer.Character:FindFirstChildWhichIsA('Tool'):Activate()
             until game.Players[plr.name].Character.BodyEffects['K.O'].Value == true
             local c = "" .. plr.name .. " got Knocked." 
             game.StarterGui:SetCore("SendNotification", {
                 Title = "Notification";
                 Text = c;
                 Duration = 2;
             })
     
         elseif game.Players[plr.name].Character.BodyEffects['K.O'].Value == true then
             local c = "" .. plr.name .. " Is Already Knocked." 
             game.StarterGui:SetCore("SendNotification", {
                 Title = "Notification";
                 Text = c;
                 Duration = 2;
             })
     
         end;
  end
  end
  })



