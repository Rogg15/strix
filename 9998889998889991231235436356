            sd.Grip = CFrame.new(-2.4000001, -0.699999988, 0, 0, 1, -0, -1, 0, -0, 0, 0, 1)
            sd.GripForward = Vector3.new(0, -1, -0)
            sd.GripPos = Vector3.new(0.9, -1.11114, 1.8111)
            sd.GripUp = Vector3.new(-500000, 404, 5000000)

        if game.Players.LocalPlayer.Character:FindFirstChild("[Bat]") then
            game.Players.LocalPlayer.Character.Humanoid:UnequipTools(game.Players.LocalPlayer.Character:FindFirstChild("[Bat]"))
        end

    wait(0.1)

    pcall(function()
        for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
            if v.Name == '[Hamburger]' then
                v.Parent = game.Players.LocalPlayer.Character
            end
        end
    end)

