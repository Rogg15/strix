 section:Button({Text = "Disable",Callback = function() 

    _G.toggle = false 
    pcall(function()
        for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
            if v.Name == '[SprayCan]' then
                v.Parent = game.Players.LocalPlayer.Character
            end
        end
    end)
    LP = game:GetService("Players").LocalPlayer
        
    for i,v in pairs(LP.Character:GetDescendants()) do
    if v.Name == "[SprayCan]" and v.Parent then 
    v.Parent = LP
    end
    end

    game.Players.LocalPlayer.PlayerGui.MainScreenGui.Enabled = true
    game:GetService("CoreGui").RobloxGui.Backpack.Hotbar.Visible = true
    game:GetService("CoreGui").RobloxGui.Backpack.Visible = true


 end
