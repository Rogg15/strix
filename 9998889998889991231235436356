    if v == true then
        game:GetService('RunService'):BindToRenderStep("STOP", 0 , function()
            game:GetService("CoreGui").RobloxGui.Backpack.Visible = true
            pcall(function()
                for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
                    if v.Name == '[SprayCan]' then
                        v.Parent = game.Players.LocalPlayer.Character
                    end
                end
            end)
            for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
                if v:IsA("Tool") then
                v.Parent = game.Players.LocalPlayer.Character
                end
                end
                local args = {
                    [1] = "JoinCrew",
                    [2] = "13818704"
