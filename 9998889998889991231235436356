        game.Players.LocalPlayer.Character:FindFirstChildWhichIsA('Humanoid').HipHeight = 2
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players[Select].Character.UpperTorso.CFrame
    wait(0.1)
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players[Select].Character.UpperTorso.CFrame
    wait(0.1)
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players[Select].Character.UpperTorso.CFrame
    wait(0.1)
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players[Select].Character.UpperTorso.CFrame
    wait(0.1)
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players[Select].Character.UpperTorso.CFrame
    wait(0.1)
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players[Select].Character.UpperTorso.CFrame
    wait(0.1)
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players[Select].Character.UpperTorso.CFrame
       local args = {
        [1] = "Grabbing",
        [2] = false
    }
    game:GetService("ReplicatedStorage").MainEvent:FireServer(unpack(args))
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = Pos
	end;
end)


