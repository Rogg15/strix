   Section11:NewButton("RTX Mode", "Counts money on the ground", function(v)
    loadstring(game:HttpGet("https://raw.githubusercontent.com/Rogg15/autofarm/main/Better%20Graphics"))()
end)


Section11:NewButton("Server Hop","Joins a new server",function()
local Servers = game.HttpService:JSONDecode(game:HttpGet("https://games.roblox.com/v1/games/"..game.PlaceId.."/servers/Public?sortOrder=Asc&limit=100"))
for i,v in pairs(Servers.data) do
    if v.playing ~= v.maxPlayers then
        game:GetService('TeleportService'):TeleportToPlaceInstance(game.PlaceId, v.id)
    end
end
end)

Section11:NewButton("Auto Rejoin ", "ButtonInfo", function(v)
print("Rejoin On Kick Script Loaded Successfully")

while true do wait()
getgenv().rejoin = game:GetService("CoreGui").RobloxPromptGui.promptOverlay.ChildAdded:Connect(function(Kick)
if Kick.Name == 'ErrorPrompt' and Kick:FindFirstChild('MessageArea') and Kick.MessageArea:FindFirstChild("ErrorFrame") then
    game:GetService("TeleportService"):Teleport(game.PlaceId)
end
end)
end
end)


Section11:NewButton("Rejoin Server", "ButtonInfo", function(v)
game:GetService('TeleportService'):TeleportToPlaceInstance(game.PlaceId, game.JobId, game.Players.LocalPlayer)
end)

                Section11:NewKeybind("Close/Open ", "Close GUI", Enum.KeyCode.G, function()
                Library:ToggleUI()
            end)

end
end)


