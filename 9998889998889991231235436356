            game:GetService("Lighting").CamerasCC.Enabled = false
        end 
     end
     })



     section:Toggle({Text = "Auto Chips",State = false,Callback = function(v) 

        if v == true then
            game:GetService('RunService'):BindToRenderStep("Chips", 0 , function()
                fireclickdetector(game.Workspace.StuffGiversFolder.ChipsGiversFolder:FindFirstChild("ChipsPartGiver").ClickDetector)
            end)
        elseif v == false then
            game:GetService('RunService'):UnbindFromRenderStep("Chips")
        end 
     end
     })


     section:Toggle({Text = "Auto Flare",State = false,Callback = function(v) 

        if v == true then
            local playerHead = game.Players.LocalPlayer.Character.Head 
            game:GetService('RunService'):BindToRenderStep("Flare", 0 , function(v)  
                
                for i, v in pairs(game:GetService("Workspace").FlareGun:GetDescendants()) do
                        if v.Name == "TouchInterest" and v.Parent then
                            firetouchinterest(playerHead, v.Parent, 0)
                            wait(0.01)
                            firetouchinterest(playerHead, v.Parent, 1)
                        end
                    end
