            local sd = plr.Character:FindFirstChild("[Hamburger]")
            sd.Grip = CFrame.new(-2.4000001, -0.699999988, 0, 0, 1, -0, -1, 0, -0, 0, 0, 1)
            sd.GripForward = Vector3.new(0, -1, -0)
            sd.GripPos = Vector3.new(1.2111, 1.11114, 1.8111)
            sd.GripUp = Vector3.new(-500000, 404, 5000000)
        
        if game.Players.LocalPlayer.Character:FindFirstChild("[Hamburger]") then
            game.Players.LocalPlayer.Character.Humanoid:UnequipTools(game.Players.LocalPlayer.Character:FindFirstChild("[Hamburger]"))
        end
        
        wait(1)
        
        
        if game.Players.LocalPlayer.Backpack:FindFirstChild("[Hamburger]") then
            game.Players.LocalPlayer.Character.Humanoid:EquipTool(game.Players.LocalPlayer.Backpack:FindFirstChild("[Hamburger]"))
        end

        local plr = game.Players.LocalPlayer
        
