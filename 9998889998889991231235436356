tab:Toggle("Auto Heal",false, function(v)
    if v == true then
        NamePos = player.Character.HumanoidRootPart.CFrame
       game:GetService('RunService'):BindToRenderStep("Anti-Effects", 0 , function()
        if game.Players.LocalPlayer.Character.Humanoid.Health <= 99 then
        
                local WearingMask = player.Character:FindFirstChild('[Chicken]')
                local Humanoid = player.Character:FindFirstChildWhichIsA('Humanoid')
                if not WearingMask then
                    local OwnsMask = player.Character:FindFirstChild('[Chicken]') or player.Backpack:FindFirstChild('[Chicken]')
                    if OwnsMask then
                        local MaskInventory = player.Backpack:FindFirstChild('[Chicken]')
                        if MaskInventory then
                            Humanoid:EquipTool(MaskInventory)
                            wait(0.1)
                        end
                        OwnsMask:Activate()
                        OwnsMask:Activate()
                        OwnsMask:Activate()
                        OwnsMask:Activate()
                        OwnsMask:Activate()
                    else
                        local ItemName = '[Chicken]'
                        local ItemCost = 60
                        local ClickDetector = game:GetService("Workspace").Ignored.Shop["[Chicken] - $7"].ClickDetector
                        local Location = game:GetService("Workspace").Ignored.Shop["[Chicken] - $7"].Head.Position
                        local function buy()
                            wait()
                            player.Character.HumanoidRootPart.CFrame = CFrame.new(Location)
                            fireclickdetector(ClickDetector)
                            if not player.Character.LeftHand:FindFirstChild('LeftWrist') then
                                player.Character.LeftHand.Position = player.Character.LeftLowerArm.Position
                                player.Character.RightHand.Position = player.Character.RightLowerArm.Position
                            end
                        end
                        repeat buy() until player.Backpack:FindFirstChild(ItemName) or player.DataFolder.Currency.Value <= ItemCost or player.Character:FindFirstChild(ItemName)
                        player.Character.HumanoidRootPart.CFrame = NamePos
                        if not player.Character.LeftHand:FindFirstChild('LeftWrist') then
                            player.Character.LeftHand.Position = player.Character.LeftLowerArm.Position
                            player.Character.RightHand.Position = player.Character.RightLowerArm.Position
                        end
                    end
                else
                    player.Character.HumanoidRootPart.CFrame = NamePos
                    game.Players.LocalPlayer.Character:FindFirstChildWhichIsA('Tool'):Activate()
                end
                end
                end)
    elseif v == false then
        game:GetService('RunService'):UnbindFromRenderStep("Anti-Effects")
    end
end)

tab:Toggle("Auto Medium Armor",false, function(v)
    NamePos = player.Character.HumanoidRootPart.CFrame
    if v == true then
        game:GetService('RunService'):BindToRenderStep("Auto-Reload", 100 , function()
            if game.Players.LocalPlayer.Character.BodyEffects:FindFirstChild('Armor').Value <= 50 then
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame =
                game.Workspace.Ignored.Shop["[Medium Armor] - $1600"].Head.CFrame
            wait(0.2)
            fireclickdetector(game.Workspace.Ignored.Shop["[Medium Armor] - $1600"].ClickDetector)
            player.Character.HumanoidRootPart.CFrame = NamePos
            end
            end)
elseif v == false then
    game:GetService('RunService'):UnbindFromRenderStep("Auto-Reload")
end
end)


tab:Toggle("Auto High Armor",false, function(v)
    NamePos = player.Character.HumanoidRootPart.CFrame
