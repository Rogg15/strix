game:GetService('Players').LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-225.8, 18.8493, -77.3)
fireclickdetector(game:GetService("Workspace").Ignored.Shop["[StopSign] - $300"].ClickDetector)
wait(0.8)
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = Pos
end)

























local MainSection = Main:NewSection("Teleport")



function findplayer(stringg)
        for _, v in pairs(game.Players:GetPlayers()) do
                if stringg:lower() == (v.Name:lower()):sub(1, #stringg) then
                        return v
                elseif stringg:lower() == (v.DisplayName:lower()):sub(1, #stringg) then
                        return v
                end
        end
end

    MainSection :NewTextBox("To Player", "Teleports to the player", function(txt)
    	local plr = findplayer(txt)
        if plr then
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = plr.Character.HumanoidRootPart.CFrame
    	end
    end)


MainSection:NewButton("Zone 1", "Counts money on the ground", function(v)
    game:GetService('Players').LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(637.745, 38.25, -64)
end)

MainSection:NewButton("Zone 2", "Counts money on the ground", function(v)
    game:GetService('Players').LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-130.274, -56.1623, 144.732)
end)



local Main = Window:NewTab("Cash")
local Section1= Main:NewSection("Cash Stuff")

Section1:NewToggle("Start Dropping", "On Loop!", function(state)

    if state then
        while true do

            local args = {
                [1] = "DropMoney",
                [2] = 10000

            }

            game:GetService("ReplicatedStorage").MainEvent:FireServer(unpack(args))
            wait(15)



            local args = {
                [1] = "DropMoney",
                [2] = 10000

            }

            game:GetService("ReplicatedStorage").MainEvent:FireServer(unpack(args))
            wait()


        end
    else
        print("Is on Endless Loop Drop")
    end
end)
    Section1:NewButton("Cash Say Check", "TypedFunction", function(v)
            local amt = 0
            for i,v in pairs(game.Workspace.Ignored.Drop:GetChildren()) do
                if v.Name == "MoneyDrop" then
                    local cur = string.split(v.BillboardGui.TextLabel.Text,"$")[2]
                    if string.match(cur,',') then
                        amt = amt + cur:gsub("%,","")
                    else
                        amt = amt + cur
                    end
                    print(string.split(v.BillboardGui.TextLabel.Text,'$')[2])
                end
            end
            function fornum(amt)
                amt = tostring(amt)
                return amt:reverse():gsub("%d%d%d", "%1,"):reverse():gsub("^,", "")
            end
            game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer("$"..fornum(amt), 'All')
        
    
end) 

Section1:NewButton("Cash count", "Counts money on the ground", function(v)
    loadstring(game:HttpGet("https://raw.githubusercontent.com/LPrandom/lua-projects/master/dhccounter.lua"))()
end)
Section1:NewButton("Auto Delete Cash", "Counts money on the ground", function(v)

while wait() do
game.Workspace.Ignored.Drop.MoneyDrop:Destroy()


game.Workspace.Ignored.Drop.MoneyDrop:Destroy()


game.Workspace.Ignored.Drop.MoneyDrop:Destroy()


game.Workspace.Ignored.Drop.MoneyDrop:Destroy()


game.Workspace.Ignored.Drop.MoneyDrop:Destroy()

wait(5)
end;
end)

