    section:Button({Text = "Remove Cuffed",Callback = function() 
            game:GetService("Players").LocalPlayer.PlayerGui.MainScreenGui.TimerJail.TextTransparency = 1
        for _, v in next, game:GetService("Players").LocalPlayer.Character:FindFirstChildOfClass("Humanoid"):GetPlayingAnimationTracks() do
            if v.Animation then
                v:Stop()
            end
        
        end
        game:GetService("Workspace").Players.LocalPlayer.Character.Humanoid.Jump = true
        game:GetService("Workspace").Players.LocalPlayer.BodyEffects.Cuff:Destroy()
    end
