                    if string.match(cur,',') then
                        amt = amt + cur:gsub("%,","")
                    else
                        amt = amt + cur
                    end
                    print(string.split(v.BillboardGui.TextLabel.Text,'$')[2])
                end
            end
            function fornum(amt)
