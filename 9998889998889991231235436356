            if v:IsA("Tool") and v.Name == "potion" then
                game.Players.LocalPlayer.Character.Humanoid:EquipTool(game.Players.LocalPlayer.Backpack:FindFirstChildOfClass("Tool"))
                wait(1)
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(_G.savedhumanoidpos)

            game.Players.LocalPlayer.Character.Name = 'Deleted'
            wait(6)
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(_G.savedhumanoidpos)
                    
        end
        end
    
        end
            end
